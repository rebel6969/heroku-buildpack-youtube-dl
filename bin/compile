pip install --upgrade hachoir heroku heroku3 hexsticker httplib2 idna loguru meval multidict glob2 requests tgcrypto
pip install --upgrade oauthlib pillow psutil psycopg2 psycopg2-binary py-crypt pyaes pyasn1 pyasn1-modules pybase64 pycparser
pip install --upgrade pydownload pyfiglet pylast pymongo pymongo[srv] "pymssql<3.0" pyowm python-barcode utilities-package
pip install --upgrade python-dotenv python-git pytube  python-magic python-sql pythondialog pytz pyyandextranslateapi
pip install --upgrade qrcode redis regex requests requests-oauthlib rsa selenium setuptools simple-crypt six smmap2
